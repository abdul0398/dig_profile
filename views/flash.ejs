<%if(success && success.length){%>
<div  class="text-white alert alert-success alert-dismissible fade show w-75 mx-auto"
    role="alert">
    <strong><%=success%></strong>
    <button type="button" class="" data-bs-dismiss="alert"
        aria-label="Close"></button>
</div>
<%}%>
<%if(error && error.length){%>
<div class="alert text-white alert-danger alert-dismissible fade show w-75 mx-auto"
    role="alert">
    <strong><%=error%></strong>
    <button type="button" style="color: aliceblue;" class="btn-close" data-bs-dismiss="alert"
        aria-label="Close"></button>
</div>
<%}%>